package example;

import java.util.Arrays;

public class Bitwise {

	public static void main(String[] args) {
		
//     	int a=4;
//     	
//     	int b=6;
//     	
//     	//System.out.println(++a + b++);
//		System.out.println("A << B: "+(a<<b));
//		System.out.println(" A>> B: "+(a>>b));
//		System.out.println("A >>> B: "+(a>>>b));
		
		char [] w= {'A','B','n','A'};
		for(int i=0;i<w.length;i++)
		{
			System.out.println(w[i]);
		}
		System.out.println(".............");
		Arrays.sort(w);
		for(int i=0;i<w.length;i++)
		{
			System.out.println(w[i]);
		}
		for(int i=0;i>w.length;i--)
		{
			System.out.println(w[0])
		}
		

		
	}

}
